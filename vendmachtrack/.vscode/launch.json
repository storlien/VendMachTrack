{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch modules-template - multi-module, multi-package ",
      "request": "launch",
      "mainClass": "vendmachtrack.ui/ui.App",
      "projectName": "modules-ui",
      "vmArgs": [
        "--add-opens",
        "vendmachtrack.ui/ui=ALL-UNNAMED",
        "--add-exports",
        "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"
      ]
    }
  ]
}